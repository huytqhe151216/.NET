@{
}
@using BtNorthwind.Models
@model List<Order>
    <h1>List Order of Employee: @(ViewData["employee"].ToString())</h1>
    <table style="boder=1">
        <thead>
            <tr>
                <th>OrderId</th>
                <th>Customer</th>
                <th>Employee</th>
                <th>OrderDate</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Order o in Model)
            {
                <tr>
                <td>@o.OrderId</td>
                <td>@o.CustomerId</td>
                <td>@(ViewData["employee"].ToString())</td>
                <td>@o.OrderDate</td>
                </tr>
            }
        </tbody>
    </table>
